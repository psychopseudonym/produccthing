class Product{
   private String pID2;
   private String sPrice2;
   private String pPrice2;
   private String textD2;
   private int numB2;
   private int numS2;
   private int numOH2;
   private int profitG2;
   
   public Product(String pID, String sPrice, String textD, String pPrice, int numB, int numS, int numOH, int profitG){
      pID = this.pID;
      sPrice = this.sPrice; 
      textD = this.textD;
      pPrice = this.pPrice;
      numB = this.numB;
      numS = this.numS;
      numOH = this.numOH;
      profitG = this.profitG;
   }
}
