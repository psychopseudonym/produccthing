class Product{
   private String pID;
   private String sPrice;
   private String pPrice;
   private String textD;
   private int numB;
   private int numS;
   private int numOH;
   private int profitG;
   
   public Product(String pID, String sPrice, String textD, String pPrice, int numB, int numS, int numOH, int profitG){
      pID = this.pID;
      sPrice = this.sPrice; 
      textD = this.textD;
      pPrice = this.pPrice;
      numB = this.numB;
      numS = this.numS;
      numOH = this.numOH;
      profitG = this.profitG; 
   }
   
   public String getPID(){
      return pID;
   }
   public void setPID(String pID){
      pID = this.pID();
   }
   public String getSPrice(){
      return sPrice;
   }
   public void setSPrice(String sPrice){
      sPrice = this.sPrice();
   }
   public String getPPrice(){
      return pPrice;
   }
   public void getPPrice(String pPrice){
      pPrice = this.pPrice();
   }
   public String getTextD(){
      return textD;
   }
   public void setTextD(String textD){
      textD = this.textD();
   }
   public int getNumB(){
      return numB;
   }
   public void setNumB(int numB){
      numB = this.numB();
   }
   public int getNumS(){
      return numS;
   }
   public void setNumS(int numS){
      numS = this.numS();
   }
   public int getNumOH(){
      return numOH;
   }
   public void setNumOH(int numOH){
      numOH = this.numOH();
   }
   public int getProfitG(){
      return profitG;
   }
   public void setProfitG(int profitG){
      profitG = this.profitG();
   }
}
